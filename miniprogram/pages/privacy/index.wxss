.page-container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;

  height: 100%;
  /* margin-top: 150rpx; */
  padding-top: 50rpx;
}

.scroll-container {
  height: 900rpx;
  width: 100%;
  margin-top: 50rpx;
  margin-bottom: 50rpx;

  box-shadow: 1px 1px 4px rgba(14, 27, 25, 0.05);
}

.private-items {
  background: white;
  border-radius: 8px;
  padding: 10px;
  margin-bottom: 20px;
  box-shadow: 2px 2px 8px rgba(0, 0, 0, 0.05);
}

.private-items text:first-child {
  font-size: large;
  font-weight: bold;
  text-align: center;
  width: 100%;
  margin-bottom: 40rpx;
}

.page-container {
  font-size: 30rpx;
  margin: 50rpx;
}

.ok-or-not-container {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  gap: 50rpx;

  margin-top: 50rpx;
}